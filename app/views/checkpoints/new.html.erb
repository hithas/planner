<h1>New Checkpoint</h1>

<form action="/create_checkpoint">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" type="text" />
  </div>
  <div>
    <label for="goal_id">goal_id</label><br />
    <input id="goal_id" name="goal_id" type="text" />
  </div>
  <div>
    <label for="num">num</label><br />
    <input id="num" name="num" type="text" />
  </div>
  <div>
    <label for="completed">completed</label><br />
    <input id="completed" name="completed" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>

